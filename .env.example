# Development Environment Variables
# These are defaults, you can override them

# Application
APP_ENV=dev
APP_DEBUG=1
APP_SECRET=dev_secret_not_for_production

# Build target
BUILD_TARGET=development

# PostgreSQL
POSTGRES_DB=app
POSTGRES_USER=app
POSTGRES_PASSWORD=app_password

# Nginx
NGINX_PORT=8000
NGINX_CONFIG=default.conf
FRONTEND_BUILD_PATH=./frontend

# Xdebug
XDEBUG_MODE=debug,develop,coverage
